#include<stdio.h>
int main()
 {
  int n,i,flag;
  printf("Enter the number:");
  scanf("%d",&n);
  for(i=2;i<=n/2;i++)
   {
    if(n%i==0)
     flag=0;
    }
    if(n==1)
    printf("1 is neither prime nor composite");
    else if(flag==0)
      printf("Entered number %d is not prime",n);
    else 
      printf("Entered number %d is prime",n);
    
 }
